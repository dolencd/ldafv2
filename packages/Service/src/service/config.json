{
    "name": "testService",
    "src": "",
    "plugins": [
        {
            "name": "protobuf",
            "src_npm": "@ldafv2/plugin-protobuf",
            "config": {
                "protoFiles": [
                    "./main.proto"
                ]
            }
        }
    ],
    "methods": [
        {
            "name": "add",
            "type": "default",
            "pluginConfig": {
                "protobuf":{
                    "inputSchema": "simpleNumber",
                    "outputSchema": "simpleNumber"
                }
            } 
        },
        {
            "name": "hashData",
            "type": "default",
            "pluginConfig": {
                "protobuf":{
                    "inputSchema": "hashDataRequest",
                    "outputSchema": "hashDataResponse"
                }
            } 
        },
        {
            "name": "getBlockNumber",
            "type": "default",
            "pluginConfig": {
                "protobuf":{
                    "outputSchema": "simpleNumber"
                }
            } 
        },
        {
            "name": "updateValue",
            "type": "noRes",
            "pluginConfig": {
                "protobuf":{
                    "inputSchema": "simpleNumber"
                }
            } 
        }
    ]
}